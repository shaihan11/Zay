<!doctype html>
<html lang="{{ request.locale.iso_code }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">

    <link rel="canonical" href="{{ canonical_url }}">

    {% comment %} favicon {% endcomment %}
    <link rel="shortcut icon" type="image/x-icon" href="{{ 'favicon.ico' | asset_url}}">
        
    {% comment %} stylesheets {% endcomment %}
    <link rel="stylesheet" href="{{ 'bootstrap.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'slick.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'slick-theme.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'templatemo.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'custom.css' | asset_url }}">

    <link rel="stylesheet" href="{{ 'fontawesome.css' | asset_url }}">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Roboto:wght@100;200;300;400;500;700;900&display=swap">

    {{ content_for_header }}
  </head>

  <body class="">

    {% section 'announcement-bar' %}
    {% section 'header' %}

    <main id="MainContent" class="content-for-layout focus-none" role="main" tabindex="-1">
      {{ content_for_layout }}
    </main>

    {% section 'footer' %}


    <script src="{{ 'jquery-1.11.0.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'jquery-migrate-1.2.1.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'bootstrap.bundle.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'templatemo.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'custom.js' | asset_url }}" defer="defer"></script>

  </body>
</html>
    